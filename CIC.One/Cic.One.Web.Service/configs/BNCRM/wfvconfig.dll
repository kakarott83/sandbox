    <WfvEntry>
      <syscode>wfvapptmt_listenotfiltered</syscode>

      <entrytype>0</entrytype>
      <befehlszeile>apptmt_liste</befehlszeile>
      <customentry>
        <detailsyscode>wfvapptmt_change_dashboard</detailsyscode>
        <createsyscode>wfvapptmt_change_dashboard</createsyscode>
        <title>Termine</title>
        <icon>disk</icon>
        <filters>
          <Filter>
            <fieldname>APPTMT.STARTDATE</fieldname>
            <filterType>DateGE</filterType>
            <description>Von</description>
            <valueType>DATE</valueType>
            <fieldType>CALENDAR</fieldType>
            <value>Date.parse("1")</value>
          </Filter>
          <Filter>
            <fieldname>APPTMT.ENDDATE</fieldname>
            <filterType>DateLE</filterType>
            <description>Bis</description>
            <valueType>DATE</valueType>
            <fieldType>CALENDAR</fieldType>
            <value>Date.parse("1").add(6).months()</value>
          </Filter>

          <Filter>
            <fieldname>PEUNI.SYSPEROLE</fieldname>
            <filterType>Equal</filterType>
            <groupName>b</groupName>
            <orFilter>false</orFilter>
            <andFilter>false</andFilter>
            <description>Besitzer</description>
            <xprocode>PUSER</xprocode>
            <valueType>LONG</valueType>
            <fieldType>MULTISELECT</fieldType>
          </Filter>
        </filters>
        <sortfields>MDBS_TOORATIME( APPTMT.STARTTIME,APPTMT.STARTDATE)</sortfields>
        <sortorder>ASC</sortorder>

      </customentry>
    </WfvEntry>
    

