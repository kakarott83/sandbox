      <syscode>wfvzahlungsplan_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>zahlungsplan_liste</befehlszeile>
      <customentry>
        <title>Tilgungsplan</title>
        <sortfields>slpos.valuta</sortfields>
        <sortorder>asc</sortorder>
        <instantSearch>1</instantSearch>
        <viewmeta>
          <query>
            <table>SLPOS</table>
            <pkey>SLPOS.SYSSLPOS</pkey>
            <tables>CIC.SLPOS, CIC.SLPOSTYP, CIC.SLLINK, CIC.SL</tables>
            <fields>slpos.*, slpostyp.name tilgungstyp</fields>
			<where>slpos.sysslpostyp=slpostyp.sysslpostyp AND sl.inaktiv=0 AND slpos.inaktiv=0 AND slpos.syssl=sllink.syssl AND sl.syssl=slpos.syssl</where>
		  </query>
