        <filterfields>NUMMER, ABWICKLUNGSORT, STAMMNUMMER, FABRIKAT, HERSTELLER, SCHILD, VIN, BERATER, kontfirmaname, kontfirmavorname,ATTRIBUT</filterfields>
        <sortfields>erfassung</sortfields>
