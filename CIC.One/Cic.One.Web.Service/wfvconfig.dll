    <!-- FÜR HCE -->
    <WfvEntry>
      <syscode>wfv_finanzierung_nkk_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>finanzierung_nkk_liste</befehlszeile>
      <customentry>
        <title>Finanzierung Bestand</title>
        <detailsyscode>wfv_nkkdetail_dash</detailsyscode>
        <filterfields>(select value from ddlkppos where code='KLINART' and id=klinie.sysklinart),nkk.ernummer,person.name,person.code,prproduct.name,obtypmap.tsn,obtypmap.hsn,ob.fgnr,obtyp.bezeichnung,ob.fabrikat,ob.specification</filterfields>
        <internalfilter>NKK.SYSNKKTYP=12 and ( nkk.zustand!='abgerechnet' or nkk.zustand is null or (nkk.zustand='abgerechnet' and nkk.zustandam BETWEEN trunc(sysdate-31) and trunc(sysdate+1)- 1 / 86400))</internalfilter>
        <sortfields>NKK.SYSNKK</sortfields>
        <sortorder>DESC</sortorder>
        <instantSearch>1</instantSearch>
        <viewmeta>
          <query>
            <table>NKK</table>
            <pkey>NKK.SYSNKK</pkey>
            <tables>CIC.NKK NKK, CIC.OB OB, CIC.OBBRIEF OBBRIEF, CIC.NKONTO NKONTO, CIC.WAEHRUNG WAEHRUNG, CIC.NKKTYP NKKTYP, CIC.PRPRODUCT PRPRODUCT, CIC.RVT RVT, CIC.PERSON PERSON, obtypmap, obtyp, CIC.PEROLE PEROLE, CIC.KLINIE</tables>
            <fields>(select value from ddlkppos where code='KLINART' and id=klinie.sysklinart) RAHMEN,ob.hersteller,trim(nkk.ernummer) ernummer,nkk.konto, obtyp.bezeichnung modell, OBBRIEF.FIDENT, OB.FGNR, OB.FABRIKAT,obtypmap.hsn, obtypmap.tsn,ob.specification laufnummer,NKK.SYSNKK, NKK.SYSNKONTO, NKK.BEGINN, NKK.SYSNKKTYP, NKK.SYSRVT, NKK.SYSPRPRODUCT, NKK.ZUSTAND, NKK.ENDE, NKK.NOMINAL, NKK.ENDEVF, NKK.AUSZAHLUNGP, NKK.AUSZAHLUNG, NKK.AUSZAHLUNGAM, NKK.SYSPRINTSETVF, NKK.SYSPRINTSETAF, NKK.SYSPRTLGSET, NKK.RESTSCHULD SALDO,  OB.SYSOB, OB.OBJEKT OBJEKT, WAEHRUNG.CODE WAEHRUNGCODE,  PRPRODUCT.NAME PRODUCTNAME,  RVT.SYSPERSON,  PERSON.CODE PERSONCODE, PERSON.NAME PERSONNAME, NKONTO.KONTO NKONTO, NKKTYP.PPY, NKKTYP.DCC, NKKTYP.bdc, NKKTYP.SUPPRESSCAP, NKKTYP.INTFIRSTDAY, NKKTYP.INTLASTDAY, NKKTYP.NAME KONTOTYP</fields>
            <where>nkk.sysklinie=klinie.sysklinie(+) and NKK.SYSNKK=OB.SYSNKK (+) AND OB.SYSOB=OBBRIEF.SYSOBBRIEF (+) AND NKK.SYSNKONTO=NKONTO.SYSNKONTO (+) AND NKK.SYSNKKTYP=NKKTYP.SYSNKKTYP (+) AND NKK.SYSPRPRODUCT=PRPRODUCT.SYSPRPRODUCT (+) AND NKK.SYSRVT=RVT.SYSRVT (+) AND RVT.SYSPERSON=PERSON.SYSPERSON (+)  AND NKONTO.SYSWAEHRUNG=WAEHRUNG.SYSWAEHRUNG (+)  and ob.sysobtyp=obtypmap.sysobtyp(+) and ob.sysobtyp=obtyp.sysobtyp(+) AND PERSON.SYSPERSON=PEROLE.SYSPERSON and obtypmap.art(+) is null</where>
            <permissioncondition> and perole.sysperole in (select sysperole from perole,roletype where roletype.code='HD' and roletype.sysroletype=perole.sysroletype connect by prior perole.sysparent=perole.sysperole start with perole.sysperole={0})</permissioncondition>
          </query>
        </viewmeta>
      </customentry>
    </WfvEntry>

