        <viewmeta>
          <query>
            <query>
				select * from (


				SELECT
				BPPROCDEF.DESCRIPTION||'|'||coalesce(ev.description,evt.description)||'|'||BPLISTENER.SYSOLTABLE||' '||ANTRAG.ANTRAG||' '||BPLISTENER.OLTABLE||' '||BPPROCDEF.PROCESSDEFCODE||' '||VTTYP.CODE||' '||VART.CODE||' '||coalesce(kd.name,it.name)||' '||coalesce(kd.vorname,it.vorname)||' '||coalesce(kd.ort,it.ort)||' '||coalesce(kd.plz,it.plz)||' '||be.vorname||' '||be.name||' '||bt.vorname||' '||bt.name||' '||antrag.attribut searchstring,
				BPLISTENER.SYSBPPROCINST,
				BPLISTENER.EVENTCODE,
				BPLISTENER.SYSBPLISTENER SYSID,
				BPPROCDEF.DESCRIPTION,
				BPPROCDEF.PROCESSDEFCODE,
				coalesce(ev.description,evt.description) STEPDESCRIPTION,
				bplistener.oltable,
				BPLISTENER.SYSOLTABLE,
				BPLISTENER.NAMEBPLANE,
				BPLISTENER.EVALUATECODE,
				BPLISTENER.PRIORITY,
				BPLISTENER.EXECSTATUS,
				coalesce(CTLANG.LANGUAGENAME,'Deutsch') SPRACHE,
				CASE
				WHEN NVL(ANTRAG.SYSKD,0) > 0
				THEN KD.MITARBEITERFLAG
				ELSE IT.MITARBEITERFLAG
				END PERSONAL,
				ANTKALK.ERSTERATE AUSZAHLUNGSDATUM,
				ANTRAG.ANTRAG NUMMER,
				VART.SYSVART VERTRAGSART,
				ANTRAG.ZUSTAND ZUSTAND,
				trunc(ANTRAG.ERFASSUNG) erfassung,
				ANTOB.FZART OBJEKTART,
				ANTRAG.BEARBEITUNG,
				ANTRAG.SYSKD,
				ANTOPTION.FLAG10 LAGERWAGEN,
				be.vorname||' '||be.name BEARBEITER,
				be.vorname bevorname,
				be.name bename,
				ANTRAG.SYSVART,
				case when COUNTRENEWVAL>0 then 'RW-Verlängerung' when TRIM(FFORM) IS NOT NULL AND COUNTRENEWVAL = 0 then 'Umschreibung' else VERTRIEBSWEG  end VERTRIEBSWEG,
				bt.vorname||' '||bt.name BERATER,
				bt.vorname btvorname,
				bt.name btname,
				ANTRAG.ATTRIBUT,
				ANTRAG.SYSVM,
				CASE
				WHEN ANTRAG.SYSVTTYP >37 and 46>ANTRAG.SYSVTTYP
				THEN VTTYP.CODE
				ELSE VART.CODE
				END BEZEICHNUNG,
				nullif(ANTOB.HERSTELLER||'/'||ANTOB.FABRIKAT,'/') FABRIKAT,
				CASE
				WHEN LENGTH(VM.ORT) > 0
				THEN VM.NAME
				|| ' '
				|| VM.VORNAME
				|| ', '
				|| VM.ORT
				ELSE VM.NAME
				|| ' '
				|| VM.VORNAME
				END HAENDLER,
				ANTKALK.SYSPRPRODUCT,
				CASE
				WHEN LENGTH(VK.ORT) > 0
				THEN VK.NAME
				|| ' '
				|| VK.VORNAME
				|| ', '
				|| VK.ORT
				ELSE VK.NAME
				|| ' '
				|| VK.VORNAME
				END VERKAEUFER,
				CASE
				WHEN NVL(ANTRAG.SYSKD,0) > 0
				THEN KD.VORNAME
				|| ' '
				|| KD.NAME
				ELSE IT.VORNAME
				|| ' '
				|| IT.NAME
				END KUNDE,
				coalesce(kd.name,it.name) kundename,
				coalesce(kd.vorname,it.vorname) kundevorname,
				CASE
				WHEN NVL(ANTRAG.SYSKD,0) > 0
				THEN KD.PLZ
				||' '
				||KD.ORT
				ELSE IT.PLZ
				||' '
				||IT.ORT
				END ORT,
				coalesce(kd.ort,it.ort) kdort,
				coalesce(kd.plz,it.plz) kdplz,
				IT.SYSIT sysIt,
				ANTKALK.AUSZAHLUNG SUMME,
				ANTRAG.SYSABWICKLUNG,
				ANTOB.BEZEICHNUNG OBJEKTBEZ,
				ANTRAG.SYSCAMP,
				BPLISTENER.syswfuser,
				antrag.erfassungsclient,
				null faellig,
				seg.sysseg segment,(select segpos.syssegpos  from segposp,segpos where segposp.syssegpos = segpos.syssegpos and segposp.sysperson = antrag.sysvm and segpos.art = 2) kategorie,camp.name kampagne,camp.syscamp kampagneid,null RSYSWFUSER,antrag.sysprchannel,awo.name ABWICKLUNGSORT,
				coalesce(HV.RESULT,' ') indicatorContent,
				0 preadFlag,
				BE.syswfuser sysbearbeiter,
				ANTRAG.SYSBERATER,
				VM.SORTKEY,
				ANTRAG.erfassungzeit,
				antrag.dateinreichungzeit,
