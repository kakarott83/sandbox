(select count(*) from antrag where  antrag.sysid in (SELECT sysid FROM peuni, perolecache WHERE area = 'ANTRAG' AND peuni.sysperole = perolecache.syschild AND perolecache.sysparent ={SYSPEROLE}) and antrag.sysit=it.sysit or antrag.syskd=it.sysperson) antragcount,
(select count(*) from vt where vt.syskd=it.sysperson and vt.sysid in (SELECT sysid FROM peuni, perolecache WHERE area = 'VT' AND peuni.sysperole = perolecache.syschild AND perolecache.sysparent ={SYSPEROLE})) vtcount,
IT.* FROM CIC.IT where
it.sysit in (SELECT sysid FROM peuni, perolecache WHERE area = 'IT' AND peuni.sysperole = perolecache.syschild AND perolecache.sysparent ={SYSPEROLE})
{FILTERCONDITIONS}
					</query>
