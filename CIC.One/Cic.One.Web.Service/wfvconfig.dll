				null ERFASSUNGZEIT,
				null dateinreichungzeit,
				angebot.dateinreichung

				FROM CIC.BPLISTENER,
				CIC.BPPROCDEF,
				--(SELECT sysAngebot, DECODE (MAX (DECODE (sla.activeflag,0,'',sla.status)) ,'Im Ziel','black','Warnung','orange','Ziel überschritten','red') SLA_ICON FROM sla GROUP BY sysAngebot) sla,
				(select /*+ RESULT_CACHE */ perole.sysperson,'banknow' result      FROM ddlkpspos,perole      WHERE ddlkpspos.value=160      AND ddlkpspos.sysddlkpcol=117      AND ddlkpspos.area ='PEROLE'      AND ddlkpspos.sysid =perole.sysperole) hv,
				CIC.bpeventdef evt,
				CIC.bpevaldef ev,
				CIC.ANGEBOT ANGEBOT,
				CIC.VART VART,
				CIC.VTTYP,
				CIC.ANGOB ANGOB,
				CIC.PERSON VM,
				CIC.PERSON VK,
				CIC.ANGKALK ANGKALK,
				CIC.PRPRODUCT PRPRODUCT,
				CIC.PERSON KD,
				CIC.IT IT,
				CIC.ANGOPTION ANGOPTION,
				cic.reminder reminder,
				CIC.SEG seg,
				CIC.camp camp,
				CIC.WFUSER bt,
				CIC.WFUSER be,
				CIC.PEROLE AWO,
				CIC.CTLANG

				WHERE

				(BPLISTENER.SUPERVISORFLAGS   =0 OR BPLISTENER.SUPERVISORFLAGS >=16) AND BPLISTENER.ISUSERTASK         = 1 AND NVL(BPLISTENER.EXECSTATUS, 0) = 0

				and BPLISTENER.OLTABLE='REMINDER' AND BPLISTENER.SYSOLTABLE= reminder.sysreminder
				and reminder.area='ANGEBOT' and reminder.sysid=ANGEBOT.sysid

				and angebot.sysvm=HV.sysperson(+)
				and BPLISTENER.SYSBPPROCDEF= BPPROCDEF.SYSBPPROCDEF(+)
				AND BPLISTENER.evaluatecode=ev.evaluatecode(+)
				and BPLISTENER.eventcode=evt.eventcode(+)

				AND (
				ANGEBOT.SYSVTTYP      = VTTYP.SYSVTTYP(+)
				AND ANGEBOT.SYSID                 = ANGKALK.SYSANGEBOT(+)
				AND ANGKALK.SYSPRPRODUCT          = PRPRODUCT.SYSPRPRODUCT (+)
				AND PRPRODUCT.SYSVART=VART.SYSVART (+)
				)
				AND (ANGEBOT.SYSID         = ANGOB.SYSANGEBOT(+))
				AND (ANGEBOT.SYSVM                = VM.SYSPERSON(+))
				AND (VM.SYSSEG                 	= SEG.SYSSEG(+))
				AND (ANGEBOT.SYSVK                = VK.SYSPERSON(+))
				AND (ANGEBOT.SYSKD                = KD.SYSPERSON(+))
				AND (ANGEBOT.SYSIT                = IT.SYSIT(+))
				AND (ANGEBOT.SYSCAMP              = CAMP.SYSCAMP(+))
				AND (ANGEBOT.SYSID                = ANGOPTION.SYSID(+))
				AND (ANGEBOT.SYSBERATER                = BT.SYSWFUSER(+))
				AND (reminder.SYSWFUSER                = BE.SYSWFUSER(+))
				AND (ANGEBOT.SYSABWICKLUNG                 = AWO.SYSPEROLE(+))
				and IT.SYSCTLANG = CTLANG.SYSCTLANG(+)
				and not(BPPROCDEF.PROCESSDEFCODE='tprc_WFA_Abklaerung' and BPLISTENER.syswfuser!={WFUSER})
				--AND SLA.SYSANGEBOT (+) = ANGEBOT.SYSID

				) bp where 1=1 {FILTERCONDITIONS}
			</query>
          </query>
        </viewmeta>
      </customentry>
    </WfvEntry>


