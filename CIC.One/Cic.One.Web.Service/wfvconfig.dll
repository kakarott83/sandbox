    </WfvEntry>
	  
    <WfvEntry>
	  <syscode>wfvkreditlinie_liste</syscode>
	  <entrytype>0</entrytype>
	  <befehlszeile>gview_liste</befehlszeile>
	  <customentry>
	    <title>Kreditlinien</title>
		<icon>disk</icon>
		<internalfilter>1=1</internalfilter>
		<instantSearch>1</instantSearch>
		<filterfields>klinart.bezeichnung</filterfields>
		<viewmeta>
  			
		  <query>
			  <postprocess>
				<PostprocessCommand type="CAS">
					<area>KLINIE</area>
					<field>s5</field>
					<command>_KLINIE:xtd('KLINIE',KLINIE:SYSKLINIE,'UTIL',0,'',2,0,today())</command>
				</PostprocessCommand>
			  </postprocess>
		  	<query>
				SELECT
				(select value from ddlkppos where code='KLINART' and id=klinie.sysklinart) art, klinie.*,klinie.sysklinie I_UKEY,
				(SELECT COUNT (1) FROM RVT, NKK, OB
				WHERE RVT.SysRVT        = NKK.SysRVT
				AND NKK.SYSNKK        = OB.SYSNKK
				AND nkk.sysklinie     = klinie.sysklinie
				AND nkk.Zustand      IN ('aktiv', 'gebucht')
				AND OB.STANDORTBRIEF IN ('Händler','angefragt Händler')
				) limitanzahl,
				(SELECT SUM (OB.BGEXTERN) FROM RVT, NKK, OB
				WHERE RVT.SysRVT        = NKK.SysRVT
				AND NKK.SYSNKK        = OB.SYSNKK
				AND nkk.sysklinie     = klinie.sysklinie
				AND nkk.Zustand      IN ('aktiv', 'gebucht')
				AND OB.STANDORTBRIEF IN ('Händler','angefragt Händler')
				) limitsumme
				FROM CIC.KLINIE, CIC.KLINART
				WHERE klinie.sysklinart = klinart.sysklinart
				AND klinie.sysklinie IN
				( SELECT DISTINCT nkk.sysklinie
				FROM CIC.NKK NKK, CIC.RVT RVT, CIC.PEROLE PEROLE
				WHERE nkk.sysklinie    IS NOT NULL
				AND nkk.sysklinie     > 0
				AND NKK.SYSRVT        = RVT.SYSRVT (+)
				AND rvt.sysperson     = perole.sysperson
				AND perole.sysperole IN
				(SELECT sysperole FROM perole, roletype
				WHERE roletype.code                 = 'HD'
				AND roletype.sysroletype          = perole.sysroletype
				CONNECT BY PRIOR perole.sysparent = perole.sysperole
				START WITH perole.sysperole       = {SYSPEROLE}
				))
			</query>
		  </query>
		  <fields>
		  	<Viewfield id="s1">
		  		<attr field="ART" label="Art" type="String"  viewtype="text" />
		  	</Viewfield>
		  	<Viewfield id="s2">
		  		<attr field="LIMITEXTERN" label="Limit-Gesamt"  type="Double" viewtype="number" suffix="€" pattern="#,##0" />
		  	</Viewfield>
		  	<Viewfield id="s3" >
		  		<attr field="LIMITANZAHL" label="Limit-Stückzahl"  type="String" viewtype="number" pattern="#,##0" />
		  	</Viewfield>
		  	<Viewfield id="s4" >
		  		<attr field="LIMITSUMME" label="Limit-Summe" type="Double"  viewtype="currency" suffix="€" />
		  	</Viewfield>
		  	<Viewfield id="s5" >
		  		<attr field="AUSLASTUNG" label="Ausschöpfung"  type="Double" viewtype="currency" suffix="€"/>
		  	</Viewfield>
		  	<Viewfield id="s6" >
		  		<attr field="BEGINN" label="Beginn" type="DateTime" viewtype="date" />
		  	</Viewfield>
		  	<Viewfield id="s7" >
		  		<attr field="ENDE" label="Ende" type="DateTime" viewtype="date" />
		  	</Viewfield>
		  </fields>
		</viewmeta>
	  </customentry>
	</WfvEntry>
