    <WfvEntry>
      <syscode>wfvbnvertrag_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>bnvertrag_liste</befehlszeile>

      <customentry>
        <title>Verträge</title>
        <icon>disk</icon>
        <detailsyscode>wfvbnvertrag_dashboard</detailsyscode>
        <filteropen>1</filteropen>


        <filters>
          <Filter>
            <fieldname>vertrag</fieldname>
            <filterType>Like</filterType>
            <description>Vertrags-Nr</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>brief</fieldname>
            <filterType>Like</filterType>
            <description>Stammnummer</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>kontname</fieldname>
            <filterType>Like</filterType>
            <description>Kontakt Firma</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>



          <Filter>
            <fieldname>FZART</fieldname>
            <filterType>Equal</filterType>
            <groupName>obart</groupName>
            <description>Objektart</description>
            <andFilter>false</andFilter>
            <orFilter>true</orFilter>
            <xprocode>FZART</xprocode>
            <valueType>STRING</valueType>
            <fieldType>SELECTCODE</fieldType>
          </Filter>
          <Filter>
            <fieldname></fieldname>
            <filterType>Like</filterType>
            <description></description>
            <valueType>STRING</valueType>
            <fieldType>PLACEHOLDER</fieldType>
          </Filter>



          <Filter>
            <fieldname>sysVttyp</fieldname>
            <filterType>Equal</filterType>
            <groupName>vart</groupName>
            <description>Vertragsart</description>
            <andFilter>false</andFilter>
            <orFilter>true</orFilter>
            <xprocode>VART</xprocode>
            <valueType>STRING</valueType>
            <fieldType>SELECT</fieldType>
          </Filter>
          <Filter>
            <fieldname>mkmodell</fieldname>
            <filterType>Like</filterType>
            <description>Marke/Modell</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname></fieldname>
            <filterType>Like</filterType>
            <description></description>
            <valueType>STRING</valueType>
            <fieldType>PLACEHOLDER</fieldType>
          </Filter>


          <Filter>
            <fieldname>zustand</fieldname>
            <filterType>Equal</filterType>
            <groupName>zust</groupName>
            <description>Zustand</description>
            <andFilter>false</andFilter>
            <orFilter>true</orFilter>
            <xprocode>VTZUSTAND</xprocode>
            <valueType>STRING</valueType>
            <fieldType>MULTISELECTCODE</fieldType>
          </Filter>
          <Filter>
            <fieldname>kennzeichen</fieldname>
            <filterType>Like</filterType>
            <description>Schild</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname></fieldname>
            <filterType>Like</filterType>
            <description></description>
            <valueType>STRING</valueType>
            <fieldType>PLACEHOLDER</fieldType>
          </Filter>

          <Filter>
            <fieldname>ATTRIBUT</fieldname>
            <filterType>Equal</filterType>
            <groupName>attr</groupName>
            <description>Attribut</description>
            <andFilter>false</andFilter>
            <orFilter>true</orFilter>
            <xprocode>ATTRIBUT</xprocode>
            <valueType>STRING</valueType>
            <fieldType>MULTISELECTCODE</fieldType>
          </Filter>
          <Filter>
            <fieldname>serie</fieldname>
            <filterType>Like</filterType>
            <description>Chassis-Nr</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname></fieldname>
            <filterType>Like</filterType>
            <description></description>
            <valueType>STRING</valueType>
            <fieldType>PLACEHOLDER</fieldType>
          </Filter>


          <Filter>
            <fieldname>sysid</fieldname>
            <filterType>Like</filterType>
            <description>ID</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>



















        </filters>
        <filterfields>VERTRAG,SYSID,Kennzeichen,Brief,Serie,bezeichnung,namekont,vornamekont</filterfields>
        <sortfields>SYSID</sortfields>
        <sortorder>DESC</sortorder>
        <searchmode>RowNum</searchmode>
        <internalfilter>{MAFLAG}>=coalesce(MITARBEITERFLAG,0)</internalfilter>
        <viewmeta>
          <query>
            <query>
              select * from
              (select VT.SYSID entityid,vt.syskd, kd.mitarbeiterflag, ob.kennzeichen,ob.brief,ob.serie,ob.bezeichnung,kd.namekont,kd.vornamekont, kd.name||' '||kd.vorname kundeName, VT.vertrag,VT.zustand,VT.attribut,VT.beginn,vt.ende,VT.sysid, ob.bezeichnung marke,kd.namekont kontfirmaname,kd.vornamekont kontfirmavorname,CASE WHEN VT.SYSVTTYP >37 and 46>VT.SYSVTTYP THEN VT.SYSVTTYP ELSE VT.sysvart END sysVttyp,ob.objektvt||' '||ob.bezeichnung mkmodell,ob.fzart,kd.namekont||' '||kd.vornamekont kontname
              from CIC.VT VT, CIC.OB OB, cic.person kd where vt.syskd=kd.sysperson(+) and vt.sysid=ob.sysvt(+)  and ob.aktivkz(+)=1) VT  where 1=1 {FILTERCONDITIONS}
            </query>
            <table>VT</table>
            <pkey>VT.SYSID</pkey>
            <tables>CIC.VT VT, CIC.OB OB, cic.person kd</tables>
            <fields>VT.SYSID entityid, kd.name||' '||kd.vorname kundeName, VT.vertrag,VT.zustand,VT.attribut,VT.beginn,vt.ende,VT.sysid, ob.bezeichnung marke,kd.namekont kontfirmaname,kd.vornamekont kontfirmavorname,CASE WHEN VT.SYSVTTYP >37 and 46>VT.SYSVTTYP THEN VT.SYSVTTYP ELSE VT.sysvart END sysVttyp</fields>
            <where>vt.syskd=kd.sysperson(+) and vt.sysid=ob.sysvt(+)  and ob.aktivkz(+)=1</where>
          </query>
        </viewmeta>

      </customentry>
    </WfvEntry>
