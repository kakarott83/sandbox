              from cic.wfmmemo,cic.wfuser,cic.wftable
              where wftable.syswftable=wfmmemo.syswfmtable and wfuser.syswfuser(+)=wfmmemo.createuser
              union all
              select 'CASESTEP' historyarea, bpcasestep.sysbpcasestep historyid, cic.MDBS_TOORATIME(bpcasestep.steptime,bpcasestep.stepdate) datum,
              bpcasestep.steptime zeit, wfuser.vorname||' '||wfuser.name  benutzer, 1 as art,
              bpcasestep.steptext titel, 0 kategorie,  bpcasestep.oltable area2, bpcasestep.sysoltable sysid2, bpcasestep.oltable area, bpcasestep.sysoltable sysid from cic.bpcasestep,
              cic.wfuser where wfuser.syswfuser=stepbyuser and bpcasestep.historyflag = 1)
              left outer join bpcasestep on (historyid=bpcasestep.sysbpcasestep and historyarea='CASESTEP'  and bpcasestep.historyflag=1)
