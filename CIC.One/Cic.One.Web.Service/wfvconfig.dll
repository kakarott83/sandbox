    <WfvEntry>
      <syscode>wfvbnpuser_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>bnpuser_liste</befehlszeile>

      <customentry>
        <title>Mitarbeiterliste</title>
        <icon>disk</icon>
        <detailsyscode>wfvbnpuser_detail</detailsyscode>
        <createsyscode>wfvbnpuser_detail</createsyscode>
        <filterfields>wfuser.NAME, wfuser.VORNAME</filterfields>
        <sortfields>name</sortfields>
        <sortorder>ASC</sortorder>
        <searchmode>RowNum</searchmode>
        <internalfilter>1=1</internalfilter>
        <instantSearch>1</instantSearch>
        <viewmeta>
          <query>
            <table>wfuser</table>
            <pkey>wfuser.syswfuser</pkey>
            <tables>puser, perole, perole hd, perole ma, roletype, wfuser </tables>
            <fields>
              (select max(rolle.id) from cic.vc_ddlkppos rolle, roleattrib,roleattribm where roleattribm.sysperole=ma.sysperole and roleattribm.sysroleattrib=roleattrib.sysroleattrib
              and rolle.code='ROLLENAUSPRAEGUNGEN' and rolle.domainid='ROLLE' and rolle.sysctlang=1 and rolle.id=to_char(roleattrib.sysroleattrib)) as rolle,
              trim((select nvl(cttstatedef.REPLACEZUSTAND,STATEDEF.ZUSTAND) from cttstatedef, STATEDEF, ctlang where STATEDEF.SYSSTATEDEF = cttstatedef.SYSSTATEDEF(+) and ctlang.sysctlang = cttstatedef.sysctlang(+)
              and STATEDEF.ZUSTAND = decode(nvl(puser.syspuser,0),0,'ePos inaktiv',nvl(ma.zustand,decode(ma.inactiveflag,0,'Aktiv','Inaktiv'))) and ctlang.isocode='{ISOCODE}')
              || ' ' ||
              (    select nvl(CTTATTRIBUTDEF.REPLACEATTRIBUT,ATTRIBUTDEF.ATTRIBUT) from CTTATTRIBUTDEF, ATTRIBUTDEF, ctlang where ATTRIBUTDEF.SYSATTRIBUTDEF = CTTATTRIBUTDEF.SYSATTRIBUTDEF(+) and ctlang.sysctlang = CTTATTRIBUTDEF.sysctlang(+)
              and ATTRIBUTDEF.ATTRIBUT = ma.attribut and ctlang.isocode='{ISOCODE}')
              ) Status,
              (select count(*) from RGR, RGM where rgr.sysrgr = rgm.sysrgr and rgr.name in ('EPOS_ADMIN') and rgm.syswfuser = wfuser.syswfuser and not exists (select 1 from wftzvar v, wftzust z where z.syswftzust=v.syswftzust and z.syswftable=2 and z.syslease = wfuser.sysperson and  z.state = 'RECHTE_FREIGABE' and v.code = to_char(rgm.sysrgr) and v.value = '0' )) adminflag,
              (select count(*) from RGR, RGM where rgr.sysrgr = rgm.sysrgr and rgr.name in ('EPOS_VERTRAGSABSCHLUSS') and rgm.syswfuser = wfuser.syswfuser and not exists (select 1 from wftzvar v, wftzust z where z.syswftzust=v.syswftzust and z.syswftable=2 and z.syslease = wfuser.sysperson and z.state = 'RECHTE_FREIGABE' and v.code = to_char(rgm.sysrgr) and v.value = '0' )) vtflag,
              puser.syspuser, wfuser.syswfuser,
              (select name from person where wfuser.sysperson=person.sysperson) name,
              (select vorname from person where wfuser.sysperson=person.sysperson) vorname,
              ma.validfrom, ma.validuntil, ma.sysperole, ma.sysparent
            </fields>
            <where>wfuser.sysperson=ma.sysperson and roletype.sysroletype=ma.sysroletype and roletype.typ=7 and wfuser.sysperson=puser.sysperson(+) and hd.sysperole=perole.sysparent and hd.sysperole=ma.sysparent</where>
            <permissioncondition> and perole.sysperole = {0}</permissioncondition>
          </query>
        </viewmeta>

      </customentry>
    </WfvEntry>


    <WfvEntry>
      <syscode>wfvbnalert_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>bnalert_liste</befehlszeile>

      <customentry>
        <title>Prüfaufgaben</title>
        <icon>disk</icon>
        <detailsyscode>wfvbnalert_detail</detailsyscode>
        <filterfields>ptype.name,ptype.description,pcontent.titel</filterfields>
        <internalfilter>ptype.completeflag=0</internalfilter>
        <sortfields>validuntil</sortfields>
        <sortorder>ASC</sortorder>
        <searchmode>RowNum</searchmode>
        <instantSearch>1</instantSearch>
        <viewmeta>
          <query>
            <table>ptype</table>
            <pkey>ptype.sysptype</pkey>
            <tables>cic.ptype ptype, cic.pcontent pcontent, cic.ptask ptask, cic.pchecker pchecker</tables>
            <fields> ptype.*,pcontent.*,case when ptype.art=10 then (select (name||' '||vorname) as plist from wfuser where syswfuser=pchecker.syswfuser) else (select listagg(name||' '||vorname,'/ ') within group (order by wfuser.name asc) as plist from wfuser,pchecker where 5>2 and wfuser.syswfuser=pchecker.syswfuser and ptask.sysptype=ptype.sysptype and pchecker.sysptask=ptask.sysptask) end plist  </fields>
            <where>ptype.art in (10,20) and ptype.status='Offen' and ptask.completeflag=0 and pchecker.sysptask=ptask.sysptask and ptask.sysptype=ptype.sysptype and ptype.sysptype=pcontent.sysptype(+) and pchecker.syswfuser= {WFUSER} AND pcontent.sysctlang = NVL((SELECT MIN(ctlang.sysctlang) FROM cic.ctlang ctlang, pcontent pc WHERE ctlang.sysctlang=pc.sysctlang AND pc.sysptype=ptype.sysptype AND ctlang.isocode = '{ISOCODE}'), (SELECT MIN(pc.sysctlang) FROM pcontent pc WHERE pc.sysptype=ptype.sysptype )) and (ptype.validuntil is null or ptype.validuntil>=sysdate  or ptype.validuntil=to_date('01.01.0111' , 'dd.MM.yyyy')) and (ptype.validfrom is null or sysdate>=ptype.validfrom  or ptype.validfrom=to_date('01.01.0111' , 'dd.MM.yyyy')) and (ptype.art=20 or (ptype.art=10 and pchecker.result is null))</where>
          </query>
        </viewmeta>

      </customentry>
    </WfvEntry>

