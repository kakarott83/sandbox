      <luceneconfig>
        <tables>
          <IndexedTableDto>
            <id>PERSON</id>
            <query>
              select * from (
              SELECT 'person' entity,it.sysit keyfield, 'IT' area,
              IT.SYSIT id,it.name||' '||vorname title,strasse ||' '||hsnr description1,plz||' '||ort description2,  decode(to_char(gebdatum,'dd.MM.yyyy'),'01.01.0111','',to_char(gebdatum,'dd.MM.yyyy'))||decode(trim(telefon),null,'',decode(gebdatum,null,'',' | ')||telefon) description3, '' peuni, it.code||' '||' '||sysit content,mitarbeiterflag
              FROM CIC.IT,
              CIC.LAND,
              CIC.STAAT
              WHERE IT.SYSLAND        = LAND.SYSLAND(+)
              AND IT.SYSSTAAT         = STAAT.SYSSTAAT(+)
              AND NVL(IT.SYSPERSON,0) = 0
              UNION ALL
              SELECT 'person' entity,PERSON.SYSPERSON keyfield, 'PERSON' area,
              PERSON.SYSPERSON id,person.name||' '||vorname title,strasse ||' '||hsnr description1,plz||' '||ort description2, decode(to_char(gebdatum,'dd.MM.yyyy'),'01.01.0111','',to_char(gebdatum,'dd.MM.yyyy'))||decode(trim(telefon),null,'',decode(gebdatum,null,'',' | ')||telefon) description3, '' peuni, person.code||' '||sysreferenz||' '||sysperson content,mitarbeiterflag

              FROM CIC.PERSON,
              CIC.LAND,
              CIC.STAAT
              WHERE PERSON.SYSLAND = LAND.SYSLAND(+) AND PERSON.FLAGKD=1 and aktivkz=1
              AND PERSON.SYSSTAAT  = STAAT.SYSSTAAT(+))
              where coalesce(mitarbeiterflag,0)=0
            </query>
            <keyField>keyfield</keyField>
            <indexid>PERSON</indexid>
            <positiveId>true</positiveId>
            <peroleFilter>false</peroleFilter>
            <areaField>area</areaField>
            <peuniFilter>IT</peuniFilter>
          </IndexedTableDto>
          <IndexedTableDto>
            <id>VERTRAG</id>
            <query>select * from (select vt.sysid keyfield,'vertrag' entity, vt.sysid id, vt.vertrag ||' '||vt.vertriebsweg title, kd.name||' '||kd.vorname description1, kd.plz||' '||kd.ort description2, vt.zustand  ||' '|| vt.attribut description3,'' peuni,kd.code||' '||kd.sysreferenz content from CIC.VT VT, cic.person kd where VT.SYSKD=KD.SYSPERSON(+) and coalesce(kd.mitarbeiterflag,0)=0 ) where 1=1</query>
            <keyField>keyfield</keyField>
            <peroleFilter>false</peroleFilter>
            <indexid>VERTRAG</indexid>
          </IndexedTableDto>
          <IndexedTableDto>
            <id>VORGANG</id>
            <query>
              select t.*,'' peuni from (
              SELECT 'vorgang' entity,ANGEBOT.SYSID keyfield,'ANGEBOT' AREA,
              ANGEBOT.SYSID ID,
              ANGEBOT.ANGEBOT ||' '||angebot.vertriebsweg title,
              coalesce(kunde.vorname,IT.vorname) ||' '|| coalesce(kunde.name,IT.NAME) description1,
              coalesce(kunde.PLZ,IT.PLZ)  || ' '||coalesce(kunde.ORT,IT.ORT) description2,
              angebot.zustand||' '||angebot.attribut description3,
              coalesce(kunde.mitarbeiterflag,it.mitarbeiterflag) mitarbeiterflag,
              PEROLE.NAME||' '||STAMMNUMMER||' '||ANGOB.FABRIKAT||' '||ANGOB.HERSTELLER||' '||ANGOB.KENNZEICHEN||' '||ANGOBBRIEF.FIDENT||' '||BERATER.VORNAME||' '||BERATER.NAME  content
              FROM CIC.ANGEBOT,
              CIC.ANGKALK,
              CIC.ANGOB,
              CIC.IT,
              CIC.PERSON HAENDLER,
              CIC.WFUSER BERATER,
              CIC.PEROLE,
              CIC.ANGOBBRIEF,
              CIC.ANGOBSICH,
              CIC.IT MH,
              CIC.PERSON KUNDE,
              CIC.SICHTYP ST
              WHERE ANGEBOT.SYSID       = ANGKALK.SYSANGEBOT(+)
              AND ANGEBOT.SYSID         = ANGOB.SYSANGEBOT(+)
              AND ANGEBOT.SYSIT         =IT.SYSIT(+)
              AND ANGEBOT.SYSBERATER    =BERATER.SYSWFUSER(+)
              AND ANGEBOT.SYSVM         =HAENDLER.SYSPERSON(+)
              AND ANGEBOT.SYSKD         =KUNDE.SYSPERSON(+)
              AND ANGEBOT.SYSID         =ANGKALK.SYSANGEBOT(+)
              AND ANGEBOT.SYSABWICKLUNG = PEROLE.SYSPEROLE(+)
              AND ANGOB.SYSOB           = ANGOBBRIEF.SYSANGOBBRIEF(+)
              AND ANGEBOT.SYSID         = ANGOBSICH.SYSANGEBOT(+)
              AND ANGOBSICH.SYSIT   = MH.SYSIT(+)
              AND (ANGEBOT.SYSANTRAG   IS NULL
              OR ANGEBOT.SYSANTRAG      = 0)
              AND ANGEBOT.ANGEBOT      IS NOT NULL
              AND INSTR(ANGEBOT.ANGEBOT,'_V', 1,1) = 0
              and angobsich.syssichtyp=ST.syssichtyp(+) and ST.RANG(+) in (120,800,130)


              UNION ALL
              SELECT 'vorgang' entity,ANTRAG.SYSID keyfield,'ANTRAG' AREA,
              ANTRAG.SYSID ID,
              ANTRAG.ANTRAG ||' '||ANTRAG.vertriebsweg title,
              coalesce(kunde.vorname,IT.vorname) ||' '|| coalesce(kunde.name,IT.NAME) description1,
              coalesce(kunde.PLZ,IT.PLZ)  || ' '||coalesce(kunde.ORT,IT.ORT) description2,
              ANTRAG.zustand||' '||ANTRAG.attribut description3,
              coalesce(kunde.mitarbeiterflag,it.mitarbeiterflag) mitarbeiterflag  ,
              PEROLE.NAME||' '||STAMMNUMMER||' '||ANTOB.FABRIKAT||' '||ANTOB.HERSTELLER||' '||ANTOB.KENNZEICHEN||' '||ANTOBBRIEF.FIDENT||' '||BERATER.VORNAME||' '||BERATER.NAME  content

              FROM CIC.ANTRAG ANTRAG,
              CIC.PERSON KUNDE,
              CIC.IT IT,
              CIC.ANTOB ANTOB,
              CIC.PERSON HAENDLER,
              CIC.WFUSER BERATER,
              CIC.PEROLE,
              CIC.ANTOBBRIEF,
              CIC.ANTOBSICH,
              CIC.PERSON MH,
              CIC.ANTKALK,
              CIC.SICHTYP ST
              WHERE ANTRAG.ANTRAG IS NOT NULL
              AND ANTRAG.SYSKD         = KUNDE.SYSPERSON(+)
              AND ANTRAG.SYSIT         = IT.SYSIT(+)
              AND ANTRAG.SYSVM         = HAENDLER.SYSPERSON(+)
              AND ANTRAG.SYSBERATER    = BERATER.SYSWFUSER(+)
              AND ANTRAG.SYSID         = ANTOB.SYSANTRAG(+)
              AND ANTRAG.SYSABWICKLUNG = PEROLE.SYSPEROLE(+)
              AND ANTOB.SYSOB          = ANTOBBRIEF.SYSANTOBBRIEF(+)
              AND ANTRAG.SYSID         = ANTOBSICH.SYSANTRAG(+)
              AND ANTOBSICH.SYSPERSON  = MH.SYSPERSON(+)
              AND ANTRAG.SYSID         = ANTKALK.SYSANTRAG(+)
              and antobsich.syssichtyp=ST.syssichtyp(+)
              and ST.RANG(+) in (120,800,130)
              and antobsich.aktivflag(+) = 1
              ) t where coalesce(t.mitarbeiterflag,0)=0
            </query>
            <keyField>keyfield</keyField>
            <peroleFilter>false</peroleFilter>
            <indexid>ANGANT</indexid>
            <areaField>area</areaField>
          </IndexedTableDto>

        </tables>
      </luceneconfig>
    </CustomerConfig>
