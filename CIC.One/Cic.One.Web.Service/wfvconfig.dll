            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>searchterms</fieldname>
            <filterType>Like</filterType>
            <description>Schlagwort</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>gedrucktam</fieldname>
            <filterType>BETWEEN</filterType>
            <groupName>GDA</groupName>
            <description>Zeitraum</description>
            <valueType>DATE</valueType>
            <fieldType>BETWEEN</fieldType>
          </Filter>
          <Filter>
            <fieldname>docinfo</fieldname>
            <filterType>Like</filterType>
            <description>Fahrgestellnummer</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>

        </filters>

        <filterfields>dmsdoc.name,dmsdoc.dateiname,dmsdoc.bemerkung,searchterms</filterfields>

        <internalfilter>1=1</internalfilter>
        <instantSearch>1</instantSearch>
        <sortfields>gedrucktam</sortfields>
        <sortorder>DESC</sortorder>
        <searchmode>NoCount</searchmode>
        <viewmeta>
          <query>
            <query>
              select * from (
              SELECT obbrief.fident searchterms,obbrief.fident docinfo, nkk.bezeichnung, dmsdoc.name, dmsdoc.dateiname, dmsdoc.bemerkung, dmsdoc.gedrucktam, dmsdoc.syswftx,  dmsdoc.sysdmsdoc
              FROM dmsdocarea,
              dmsdoc,
              perolecache,
              perole,
              dmsdocarea area2,
              NKK,
              OB,
              OBBRIEF
              WHERE
              dmsdocarea.SYSDMSDOC = dmsdoc.SYSDMSDOC
              AND dmsdocarea.area        = 'HD'
              AND area2.area            ='NKK'
              AND area2.sysid            =nkk.sysnkk
              AND area2.sysdmsdoc        =dmsdocarea.sysdmsdoc

              AND NKK.SYSNKK             =OB.SYSNKK (+)
              AND OB.SYSOB               =OBBRIEF.SYSOBBRIEF (+)
              AND dmsdocarea.sysid       =perole.sysperson
              AND perolecache.syschild   =perole.sysperole
              AND (validfrom            IS NULL
              OR TRUNC(sysdate)         >=TRUNC(validfrom))
              AND (validuntil           IS NULL
              OR TRUNC(validuntil)      >=TRUNC(sysdate))
              AND (UNGUELTIGFLAG         =0
              OR UNGUELTIGFLAG          IS NULL)
              and perolecache.sysparent =  {SYSPEROLE}

              union all
              SELECT '' searchterms,'' docinfo, '' bezeichnung, dmsdoc.name, dmsdoc.dateiname, dmsdoc.bemerkung, dmsdoc.gedrucktam, dmsdoc.syswftx,  dmsdoc.sysdmsdoc
              FROM dmsdocarea,
              dmsdoc,
              perolecache,
              perole
              WHERE
              dmsdocarea.SYSDMSDOC = dmsdoc.SYSDMSDOC
              AND dmsdocarea.area        = 'HD'
              and (select count(*) from dmsdocarea area2 where area2.sysdmsdoc=dmsdocarea.sysdmsdoc and area2.area='NKK') =0
              AND dmsdocarea.sysid       =perole.sysperson
              AND perolecache.syschild   =perole.sysperole
              AND (validfrom            IS NULL
              OR TRUNC(sysdate)         >=TRUNC(validfrom))
              AND (validuntil           IS NULL
              OR TRUNC(validuntil)      >=TRUNC(sysdate))
              AND (UNGUELTIGFLAG         =0
              OR UNGUELTIGFLAG          IS NULL)
              and perolecache.sysparent = {SYSPEROLE}
              ) dmsdoc where 1=1 {FILTERCONDITIONS}
            </query>
          </query>
        </viewmeta>
      </customentry>
    </WfvEntry>
