    <WfvEntry>
      <syscode>wfv_proc_monitor</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>gview_liste</befehlszeile>
      <customentry>
        <title>Übersicht Kennzahlen</title>
        <icon>fa-bar-char</icon>
        <filterfields>kennzahl, team, mitarbeiter</filterfields>
        <sortfields>rang;team;mitarbeiter</sortfields>
        <sortorder>ASC;DESC;DESC</sortorder>
        <searchmode>NoCount</searchmode>
        <internalfilter>1=1</internalfilter>
        <instantSearch>0</instantSearch>
        <viewmeta>
          <query>
            <!-- <table>cic.uv_bpkennzahlen</table>
            <pkey>rang</pkey>
            <tables>cic.uv_bpkennzahlen bpk, cic.wfuser, cic.perole</tables>
            <fields>kennzahl , team , mitarbeiter ,sum(wert) wert, rang</fields>
            <where>
              wfuser.sysperson = perole.sysperson
              and bpk.sysctlang = (select l.sysctlang from regvar v , regsec s , ctlang l where v.code = 'LANG' and v.area = 'WFUSER' and v.sysid = wfuser.syswfuser and v.sysregsec = s.sysregsec and s.bezeichnung = 'SALES' and l.isocode = v.wert)
              and (( bpk.bprole is null and exists (select 1 from cic.bproleusr bpu , cic.perole usr , cic.perelate rel , cic.wfuser wfu where rel.sysperole1 = bpk.sysabwicklung and rel.sysperole2 = usr.sysperole and wfu.sysperson = usr.sysperson and bpu.syswfuser = wfu.syswfuser and bpu.syswfuser = wfuser.syswfuser and bpu.teamleaderflag = 1 and bpu.namebprole like '%SALES%'))
              or ( bpk.bprole is not null and exists (select 1 from bproleusr bpu where bpu.syswfuser = wfuser.syswfuser and bpu.teamleaderflag = 1 and bpu.namebprole = bpk.bprole)))
            </where>
            <group>
              grouping sets ((rang, kennzahl, team, mitarbeiter), (rang, kennzahl, team), (rang, kennzahl)),wert having (grouping(mitarbeiter) = 1 or (grouping(mitarbeiter) = 0 and mitarbeiter is not null))
            </group>
            <permissioncondition> and perole.sysperole !={0}</permissioncondition>-->
            <query>
              SELECT /*+ parallel */ tab_1.kennzahl_id,
              tab_1.kennzahl,
              tab_1.team,
              tab_1.mitarbeiter,
              tab_1.rang,
              tab_1.bprole,
              tab_1.sysctlang,
              tab_1.sysabwicklung ,
              SUM(tab_1.wert) wert ,
              grouping(tab_1.team) teamgroup
              FROM
              (SELECT bpk.kennzahl_id ,
              bpk.kennzahl ,
              bpk.team ,
              bpk.mitarbeiter ,
              bpk.wert wert ,
              bpk.rang ,
              bpk.bprole ,
              bpk.sysctlang ,
              bpk.sysabwicklung
              FROM cic.uv_bpkennzahlen bpk,
              cic.wfuser,
              cic.perole
              WHERE wfuser.sysperson = perole.sysperson
              {FILTERCONDITIONS}
              AND bpk.sysctlang      =
              (SELECT l.sysctlang
              FROM regvar v ,
              regsec s ,
              ctlang l
              WHERE v.code      = 'LANG'
              AND v.area        = 'WFUSER'
              AND v.sysid       = wfuser.syswfuser
              AND v.sysregsec   = s.sysregsec
              AND s.bezeichnung = 'SALES'
              AND l.isocode     = v.wert
              )
              AND bpk.bprole IS NULL
              AND EXISTS
              (SELECT 1
              FROM cic.bproleusr bpu ,
              cic.perole usr ,
              cic.perelate rel ,
              cic.wfuser wfu
              WHERE rel.sysperole1   = bpk.sysabwicklung
              AND rel.sysperole2     = usr.sysperole
              AND wfu.sysperson      = usr.sysperson
              AND bpu.syswfuser      = wfu.syswfuser
              AND bpu.syswfuser      = wfuser.syswfuser
              AND bpu.teamleaderflag = 1
              AND bpu.namebprole LIKE '%SALES%'
              )
              AND perole.sysperole != {SYSPEROLE}

              UNION ALL

              SELECT bpk.kennzahl_id ,
              bpk.kennzahl ,
              bpk.team ,
              bpk.mitarbeiter ,
              bpk.wert wert ,
              bpk.rang ,
              bpk.bprole ,
              bpk.sysctlang ,
              bpk.sysabwicklung
              FROM cic.uv_bpkennzahlen bpk,
              cic.wfuser,
              cic.perole
              WHERE wfuser.sysperson = perole.sysperson
              {FILTERCONDITIONS}
              AND bpk.sysctlang      =
              (SELECT l.sysctlang
              FROM regvar v ,
              regsec s ,
              ctlang l
              WHERE v.code      = 'LANG'
              AND v.area        = 'WFUSER'
              AND v.sysid       = wfuser.syswfuser
              AND v.sysregsec   = s.sysregsec
              AND s.bezeichnung = 'SALES'
              AND l.isocode     = v.wert
              )
              AND bpk.bprole IS NOT NULL
              AND EXISTS
              (SELECT 1
              FROM bproleusr bpu
              WHERE bpu.syswfuser    = wfuser.syswfuser
              AND bpu.teamleaderflag = 1
              AND bpu.namebprole     = bpk.bprole
              )
              AND perole.sysperole != {SYSPEROLE}

              ) tab_1
              GROUP BY GROUPING SETS ( (tab_1.bprole, tab_1.sysabwicklung, tab_1.sysctlang, tab_1.rang, tab_1.kennzahl_id, tab_1.kennzahl, tab_1.team, tab_1.mitarbeiter) , (tab_1.bprole, tab_1.sysabwicklung, tab_1.sysctlang, tab_1.rang, tab_1.kennzahl_id, tab_1.kennzahl, tab_1.team) , (tab_1.sysctlang, tab_1.rang, tab_1.kennzahl_id, tab_1.kennzahl))
              HAVING (grouping(tab_1.mitarbeiter) = 1
              OR (grouping(tab_1.mitarbeiter)     = 1
              AND tab_1.mitarbeiter              IS NOT NULL))
            </query>
          </query>
          <fields>
            <Viewfield id="s1">
              <attr field="KENNZAHL" label="Kennzahl" type="String" viewtype="text" />
            </Viewfield>
            <Viewfield id="s2">
              <attr field="TEAM" label="Team" type="String" viewtype="text" />
            </Viewfield>
            <Viewfield id="s3" >
              <attr field="MITARBEITER" label="Mitarbeiter" type="String" viewtype="text" />
            </Viewfield>
            <Viewfield id="s4" >
              <attr field="WERT" label="Wert" type="Int" viewtype="number" pattern="#,##0" minimum="0" maximum="100000"/>
            </Viewfield>
          </fields>
        </viewmeta>
        <filteropen>0</filteropen>
        <filters>
          <Filter>
            <fieldname>kennzahl_ID</fieldname>
            <filterType>Equal</filterType>
            <groupName>kzahl</groupName>
