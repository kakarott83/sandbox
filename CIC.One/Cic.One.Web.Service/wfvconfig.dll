        <viewmeta>
          <query>
            <table>history</table>
            <pkey>history.HISTORYID</pkey>
            <tables>
              (select * from
              (
              select 'MEMO' historyarea, wfmmemo.syswfmmemo historyid, cic.MDBS_TOORATIME(wfmmemo.createtime,wfmmemo.createdate) datum,
              wfmmemo.createtime zeit,wfuser.vorname||' '||wfuser.name  benutzer, 0 as art,
              wfmmemo.kurzbeschreibung titel, wfmmemo.syswfmmkat kategorie, wftable.syscode area2,
              wfmmemo.syslease sysid2, 'VT' area, vt.sysid sysid
              from cic.wfmmemo,cic.wfuser,cic.wftable, vt
              where wftable.syswftable=wfmmemo.syswfmtable and wfuser.syswfuser(+)=wfmmemo.createuser
              and wfmmemo.syswfmmkat in (select cfgvar.wert
              from cic.cfgvar,cic.cfgsec,cic.cfg where cfgsec.code='MEMO__KAT_ANZEIGE'
              and cfg.code='ANTRAGSASSISTENT' and cfgsec.syscfg=cfg.syscfg and cfgvar.syscfgsec=cfgsec.syscfgsec)
              and vt.sysantrag = wfmmemo.syslease
              union all
              select 'CASESTEP' historyarea, bpcasestep.sysbpcasestep historyid, cic.MDBS_TOORATIME(bpcasestep.steptime,bpcasestep.stepdate) datum,
              bpcasestep.steptime zeit, wfuser.vorname||' '||wfuser.name  benutzer, 1 as art,
