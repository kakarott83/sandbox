    <WfvEntry>
      <syscode>wfvcrmaccountdetail_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>account_liste</befehlszeile>
      <customentry>
        <title>Kunden/Leads</title>
        <icon>men</icon>
        <detailsyscode>wfvcrmac_detail_dashboard</detailsyscode>
        <createsyscode>wfvaccount_change</createsyscode>
        <filteropen>1</filteropen>
        <filters>
          <Filter>
            <fieldname>trim(person.name)</fieldname>
            <filterType>Like</filterType>
            <description>Name</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>trim(vorname)</fieldname>
            <filterType>Like</filterType>
            <description>Vorname</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>gebdatum</fieldname>
            <filterType>DateEqual</filterType>
            <description>Geburtsdatum</description>
            <valueType>DATE</valueType>
            <fieldType>CALENDAR</fieldType>
          </Filter>


          <Filter>
            <fieldname>trim(plz)||' '||trim(ort)</fieldname>
            <filterType>Like</filterType>
            <description>PLZ / Ort</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>trim(strasse)||' '||trim(hsnr)</fieldname>
            <filterType>Like</filterType>
            <description>Strasse, Nr</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>SYSPERSON</fieldname>
            <filterType>Like</filterType>
            <description>Kunden-ID</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>


        </filters>


        <internalfilter>1=1</internalfilter>
        <filterfields>PERSON.NAME, VORNAME, STRASSE, HSNR, PLZ, ORT, PERSON.CODE, TELEFON, SYSPERSON, PERSON.SYSREFERENZ</filterfields>
        <sortfields>PERSON.sysperson</sortfields>
        <sortorder>DESC</sortorder>
        <searchmode>RowNum</searchmode>
        <viewmeta>
          <query>
            <table>PERSON</table>
            <pkey>PERSON.sysperson</pkey>
            <tables>PERSON,LAND,STAAT</tables>
            <fields>
              'PERSON' AREA,PERSON.SYSPERSON SYSID,
              PERSON.SYSPERSON,
              PERSON.CODE,
              PERSON.NAME,
              PERSON.VORNAME,
              PERSON.ZUSATZ,
              PERSON.GEBDATUM,
              PERSON.STRASSE,
              PERSON.HSNR,
              PERSON.ORT,
              PERSON.PLZ,
              PERSON.RECHTSFORMCODE,
              LAND.COUNTRYNAME,
              LAND.KENNZEICHEN,
              STAAT.STAAT,
              PERSON.TELEFON,
              PERSON.HANDY,
              PERSON.EMAIL,
              PERSON.PRIVATFLAG,
              PERSON.GESFLAG,
              PERSON.SYSKDTYP,
              PERSON.MITARBEITERFLAG,
              PERSON.SysREFERENZ
            </fields>
            <where>PERSON.SYSLAND = LAND.SYSLAND(+) AND PERSON.SYSSTAAT  = STAAT.SYSSTAAT(+)</where>
          </query>
        </viewmeta>
      </customentry>
    </WfvEntry>

