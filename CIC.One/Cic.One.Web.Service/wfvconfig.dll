    <!-- FÜR BANKNOW -->
    <WfvEntry>
      <syscode>wfvbn_finanzierung_nkk_liste</syscode>
      <entrytype>0</entrytype>
      <befehlszeile>bnfinanzierung_nkk_liste</befehlszeile>
      <customentry>
        <title>Finanzierung Bestand</title>
        <detailsyscode>wfvbn_finanzierung_dashboard</detailsyscode>
        <filterfields>obbrief.fident,nkk.konto,nkk.zustand</filterfields>
        <internalfilter>NKK.SYSNKKTYP=12 and (ob.sysvt is null or ob.sysvt=0)</internalfilter>
        <sortfields>NKK.SYSNKK</sortfields>
        <sortorder>DESC</sortorder>
        <instantSearch>0</instantSearch>
        <filteropen>1</filteropen>
        <filters>
          <Filter>
            <fieldname>trim(obbrief.fident||' '||ob.brief)</fieldname>
            <filterType>Like</filterType>
            <description>Chassisnummer</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>
          <Filter>
            <fieldname>nkk.konto</fieldname>
            <filterType>Like</filterType>
            <description>Rechnungsnummer</description>
            <valueType>STRING</valueType>
            <fieldType>INPUT</fieldType>
          </Filter>



          <Filter>
            <fieldname></fieldname>
            <filterType>SQL</filterType>
            <groupName>zart</groupName>
            <description>Status</description>
            <andFilter>false</andFilter>
            <orFilter>true</orFilter>
            <valueType>STRING</valueType>
            <fieldType>SELECT</fieldType>
            <optionvalues>Floorplan active:zustandmap.zustandextern='Floorplan active';Floorplan inactive:zustandmap.zustandextern='Floorplan inactive'</optionvalues>
          </Filter>

          <Filter>
            <fieldname>NKK.BEGINN</fieldname>
            <filterType>DateGE</filterType>
            <description>Finanzierungsbeginn</description>
            <valueType>DATE</valueType>
            <fieldType>CALENDAR</fieldType>
            <value>Date.parse("1")</value>
          </Filter>
          <Filter>
            <fieldname>NKK.ENDE</fieldname>
            <filterType>DateLE</filterType>
            <description>Finanzierungsende</description>
            <valueType>DATE</valueType>
            <fieldType>CALENDAR</fieldType>
            <value>Date.parse("1").add(6).months()</value>
          </Filter>

        </filters>
        <viewmeta>
          <query>
            <table>NKK</table>
            <pkey>NKK.SYSNKK</pkey>
            <tables>CIC.NKK NKK, CIC.OB OB, CIC.OBBRIEF OBBRIEF, CIC.NKONTO NKONTO, CIC.WAEHRUNG WAEHRUNG, CIC.NKKTYP NKKTYP, CIC.PRPRODUCT PRPRODUCT, CIC.RVT RVT, CIC.PERSON PERSON, obtypmap, obtyp, CIC.PEROLE PEROLE, CIC.PEROLE PEROLEVK, (select statedef.zustand AS zustandextern, attributdef.attribut zustandintern from statedef, state, attribut, attributdef where attribut.sysattributdef = attributdef.sysattributdef and attribut.sysstatedef = statedef.sysstatedef and attribut.sysstate = state.sysstate and state.syswftable = 180 and statedef.art = 1) zustandmap </tables>
            <fields>nkk.konto,ob.hersteller,ob.hersteller||' '||ob.fabrikat modell, OBBRIEF.FIDENT, ob.kennzeichen schild, ob.fgnr FGNR, OB.FABRIKAT,obtypmap.hsn, obtypmap.tsn,ob.specification laufnummer,NKK.SYSNKK, NKK.SYSNKONTO, NKK.BEGINN, NKK.SYSNKKTYP, NKK.SYSRVT, NKK.SYSPRPRODUCT,zustandmap.zustandextern as ZUSTAND, NKK.ENDE, NKK.NOMINAL, NKK.ENDEVF, NKK.AUSZAHLUNGP, NKK.AUSZAHLUNG, NKK.AUSZAHLUNGAM, NKK.SYSPRINTSETVF, NKK.SYSPRINTSETAF, NKK.SYSPRTLGSET, NKK.SALDO,  OB.SYSOB, OB.OBJEKT OBJEKT, WAEHRUNG.CODE WAEHRUNGCODE,  PRPRODUCT.NAME PRODUCTNAME,  RVT.SYSPERSON,  PERSON.CODE PERSONCODE, PERSON.NAME PERSONNAME, NKONTO.KONTO NKONTO, NKKTYP.PPY, NKKTYP.DCC, NKKTYP.bdc, NKKTYP.SUPPRESSCAP, NKKTYP.INTFIRSTDAY, NKKTYP.INTLASTDAY, RVT.RAHMEN, NKKTYP.NAME KONTOTYP, (select CASE WHEN ( select count(1)  from antob, nkk subnkk where antob.sysnkk = subnkk.sysnkk and subnkk.sysnkk = nkk.sysnkk) = 0 THEN 1 ELSE ( select count(1)   from antrag, antob, ob, nkk subnkk where subnkk.sysnkk = ob.sysnkk and subnkk.sysnkk = antob.sysnkk and antob.sysantrag = antrag.sysid and subnkk.sysnkktyp = 12 and nvl(subnkk.sysvt, 0) = 0 and subnkk.sysnkk = nkk.sysnkk and antrag.zustand in ('Abgeschlossen') and antrag.sysid not in ( select a.sysid from vt v, antrag a where a.sysvt = v.sysid and nvl(v.flagsaldiert, 0) = 0 )) END from nkk nkk2 where nkk2.sysnkk=nkk.sysnkk ) FINANZIERBAR</fields>
            <where>zustandmap.zustandintern=nkk.zustand and NKK.SYSNKK=OB.SYSNKK AND OB.SYSOB=OBBRIEF.SYSOBBRIEF (+) AND NKK.SYSNKONTO=NKONTO.SYSNKONTO (+) AND NKK.SYSNKKTYP=NKKTYP.SYSNKKTYP (+) AND NKK.SYSPRPRODUCT=PRPRODUCT.SYSPRPRODUCT (+) AND NKK.SYSRVT=RVT.SYSRVT (+) AND RVT.SYSPERSON=PERSON.SYSPERSON (+)  AND NKONTO.SYSWAEHRUNG=WAEHRUNG.SYSWAEHRUNG (+)  and ob.sysobtyp=obtypmap.sysobtyp(+) and ob.sysobtyp=obtyp.sysobtyp(+) AND PERSON.SYSPERSON=PEROLE.SYSPERSON and PEROLEVK.SYSPARENT=PEROLE.SYSPEROLE</where>
            <permissioncondition> and PEROLEVK.sysperole = {0}</permissioncondition>
          </query>
        </viewmeta>
      </customentry>
    </WfvEntry>

