              SELECT 'ANGEBOT' AREA,
              ANGEBOT.SYSID SYSID,
              ANGEBOT.ANGEBOT NUMMER,
              case when COUNTRENEWVAL>0 then 'RW-Verlängerung' when TRIM(FFORM) IS NOT NULL AND COUNTRENEWVAL = 0 then 'Umschreibung' else VERTRIEBSWEG  end VERTRIEBSWEG,
              ANGEBOT.ZUSTAND,
              ANGEBOT.ATTRIBUT,
              ANGEBOT.ERFASSUNG,
              ANGEBOT.AENDERUNG,
              ANGEBOT.GUELTIGBIS,
              ANGOB.BEZEICHNUNG OBJEKTBEZ,
              CASE
              WHEN ANGEBOT.SYSVTTYP >37 and 46>ANGEBOT.SYSVTTYP
              THEN ANGEBOT.SYSVTTYP
              ELSE ANGEBOT.sysvart
              END VERTRAGSART,
              ANGOB.FZART OBJEKTART,
              ANGEBOT.BEGINN,
              ANGEBOT.ENDEAM,
              ANGOB.FABRIKAT,
              ANGOB.HERSTELLER,
              coalesce(kunde.name,IT.NAME) kdname,
              coalesce(kunde.vorname,IT.vorname) kdvorname,
              coalesce(kunde.PLZ,IT.PLZ) kdplz,
              coalesce(kunde.ORT,IT.ORT) kdort,
              ANGKALK.BGINTERNBRUTTO BETRAG,
              BERATER.syswfuser BERATERID,
              BERATER.VORNAME||' '||BERATER.NAME BERATER,
              HAENDLER.SYSPERSON HAENDLERID,
              HAENDLER.NAME HAENDLERNAME,
              HAENDLER.ORT HAENDLERORT,
              PEROLE.NAME ABWICKLUNGSORT,
              MH.SYSIT SYSMH,
              MH.NAME MHNAME,
              MH.VORNAME MHVORNAME,
              ANGEBOT.ERFASSUNGSCLIENT EINGANGSKANAL,
              ANGOBBRIEF.STAMMNUMMER,
              ANGOB.KENNZEICHEN SCHILD,
              ANGOBBRIEF.FIDENT VIN,
              ANGOB.LIEFER LIEFERDATUM,
              IT.NAMEKONT kontfirmaname,
              IT.VORNAMEKONT kontfirmavorname,
              ANGEBOT.SYSIT, ANGEBOT.SYSKD, ANGEBOT.SYSCAMP,
              coalesce(kunde.mitarbeiterflag,it.mitarbeiterflag) mitarbeiterflag,
              null erfassungzeit,
              null dateinreichungzeit,
              dateinreichung
              FROM CIC.ANGEBOT,
              CIC.ANGKALK,
              CIC.ANGOB,
              CIC.IT,
              CIC.PERSON HAENDLER,
              CIC.WFUSER BERATER,
              CIC.PEROLE,
              CIC.ANGOBBRIEF,
              CIC.ANGOBSICH,
              CIC.IT MH,
              CIC.PERSON KUNDE,
              CIC.SICHTYP ST
              WHERE ANGEBOT.SYSID       = ANGKALK.SYSANGEBOT(+)
              AND ANGEBOT.SYSID         = ANGOB.SYSANGEBOT(+)
              AND ANGEBOT.SYSIT         =IT.SYSIT(+)
              AND ANGEBOT.SYSBERATER    =BERATER.SYSWFUSER(+)
              AND ANGEBOT.SYSVM         =HAENDLER.SYSPERSON(+)
              AND ANGEBOT.SYSKD         =KUNDE.SYSPERSON(+)
              AND ANGEBOT.SYSID         =ANGKALK.SYSANGEBOT(+)
              AND ANGEBOT.SYSABWICKLUNG = PEROLE.SYSPEROLE(+)
              AND ANGOB.SYSOB           = ANGOBBRIEF.SYSANGOBBRIEF(+)
              AND ANGEBOT.SYSID         = ANGOBSICH.SYSANGEBOT(+)
              AND ANGOBSICH.SYSIT   = MH.SYSIT(+)
              AND (ANGEBOT.SYSANTRAG   IS NULL
              OR ANGEBOT.SYSANTRAG      = 0)
              AND ANGEBOT.ANGEBOT      IS NOT NULL
              AND INSTR(ANGEBOT.ANGEBOT,'_V', 1,1) = 0
              and angobsich.syssichtyp=ST.syssichtyp(+) and ST.RANG(+) in (120,800,130)


              UNION ALL
              SELECT 'ANTRAG' AREA,
              ANTRAG.SYSID SYSID,
              ANTRAG.ANTRAG NUMMER,
              case when COUNTRENEWVAL>0 then 'RW-Verlängerung' when TRIM(FFORM) IS NOT NULL AND COUNTRENEWVAL = 0 then 'Umschreibung'else VERTRIEBSWEG end VERTRIEBSWEG,
              ANTRAG.ZUSTAND,
              ANTRAG.ATTRIBUT,
              ANTRAG.ERFASSUNG,
              ANTRAG.AENDERUNG,
              ANTRAG.GUELTIGBIS,
              ANTOB.BEZEICHNUNG OBJEKTBEZ,
              CASE
              WHEN ANTRAG.SYSVTTYP >37 and 46>ANTRAG.SYSVTTYP
              THEN ANTRAG.SYSVTTYP
              ELSE ANTRAG.sysvart
              END VERTRAGSART,
              ANTOB.FZART OBJEKTART,
              ANTRAG.BEGINN,
              ANTRAG.ENDEAM,
              ANTOB.FABRIKAT,
              ANTOB.HERSTELLER,
              coalesce(kunde.name,IT.NAME) kdname,
              coalesce(kunde.vorname,IT.vorname) kdvorname,
              coalesce(kunde.PLZ,IT.PLZ) kdplz,
              coalesce(kunde.ORT,IT.ORT) kdort,
              DECODE(ANTRAG.SYSPRCHANNEL, 1, ANTOB.AHKBRUTTO, 2, ANTKALK.BGINTERNBRUTTO, 0) BETRAG,
              BERATER.SYSWFUSER BERATERID,
              BERATER.VORNAME||' '||BERATER.NAME BERATER,
              HAENDLER.SYSPERSON HAENDLERID,
              HAENDLER.NAME HAENDLERNAME,
              HAENDLER.ORT HAENDLERORT,
              PEROLE.NAME ABWICKLUNGSORT,
              MH.SYSPERSON SYSMH,
              MH.NAME MHNAME,
              MH.VORNAME MHVORNAME,
              ANTRAG.ERFASSUNGSCLIENT EINGANGSKANAL,
              ANTOBBRIEF.STAMMNUMMER,
              ANTOB.KENNZEICHEN SCHILD,
              ANTOBBRIEF.FIDENT VIN,
              ANTOB.LIEFER LIEFERDATUM,
              KUNDE.NAMEKONT kontfirmaname,
              KUNDE.VORNAMEKONT kontfirmavorname,
              ANTRAG.SYSIT, ANTRAG.SYSKD, ANTRAG.SYSCAMP,
              coalesce(kunde.mitarbeiterflag,it.mitarbeiterflag) mitarbeiterflag,
              antrag.erfassungzeit,
              dateinreichungzeit,
              dateinreichung
              FROM CIC.ANTRAG ANTRAG,
              CIC.PERSON KUNDE,
              CIC.IT IT,
              CIC.ANTOB ANTOB,
              CIC.PERSON HAENDLER,
              CIC.WFUSER BERATER,
              CIC.PEROLE,
              CIC.ANTOBBRIEF,
              CIC.ANTOBSICH,
              CIC.PERSON MH,
              CIC.ANTKALK,
              CIC.SICHTYP ST
              WHERE ANTRAG.ANTRAG IS NOT NULL
              AND ANTRAG.SYSKD         = KUNDE.SYSPERSON(+)
              AND ANTRAG.SYSIT         = IT.SYSIT(+)
              AND ANTRAG.SYSVM         = HAENDLER.SYSPERSON(+)
              AND ANTRAG.SYSBERATER    = BERATER.SYSWFUSER(+)
              AND ANTRAG.SYSID         = ANTOB.SYSANTRAG(+)
              AND ANTRAG.SYSABWICKLUNG = PEROLE.SYSPEROLE(+)
              AND ANTOB.SYSOB          = ANTOBBRIEF.SYSANTOBBRIEF(+)
              AND ANTRAG.SYSID         = ANTOBSICH.SYSANTRAG(+)
              AND ANTOBSICH.SYSPERSON  = MH.SYSPERSON(+)
              AND ANTRAG.SYSID         = ANTKALK.SYSANTRAG(+)
              and antobsich.syssichtyp=ST.syssichtyp(+)
              and ST.RANG(+) in (120,800,130)
              and antobsich.aktivflag(+) = 1
              ) vc_angant  where 1=1  {FILTERCONDITIONS}
            </query>
          </query>
        </viewmeta>



      </customentry>
