              ANGOPTION.FLAG10 LAGERWAGEN,
              vk.vorname||' '||vk.name BEARBEITER,
              ANGEBOT.SYSVART,
              ANGEBOT.VERTRIEBSWEG,
              bt.vorname||' '||bt.name  BERATER,
              ANGEBOT.ATTRIBUT,
              ANGEBOT.SYSVM,
              CASE
              WHEN VTTYP.CODE IN ('CREDIT-NOW CASA','CREDIT-NOW CASA FLEX', 'CREDIT-NOW DIPLOMA')
              THEN VTTYP.CODE
              ELSE VART.BEZEICHNUNG
              END BEZEICHNUNG,
              nullif(ANGOB.HERSTELLER||'/'||ANGOB.FABRIKAT,'/') FABRIKAT,
              CASE
              WHEN LENGTH(VM.ORT) > 0
              THEN VM.NAME
              || ' '
              || VM.VORNAME
              || ', '
              || VM.ORT
              ELSE VM.NAME
              || ' '
              || VM.VORNAME
              END HAENDLER,
              ANGKALK.SYSPRPRODUCT,
              CASE
              WHEN LENGTH(VK.ORT) > 0
              THEN VK.NAME
              || ' '
              || VK.VORNAME
              || ', '
              || VK.ORT
              ELSE VK.NAME
              || ' '
              || VK.VORNAME
              END VERKAEUFER,
              CASE
              WHEN NVL(ANGEBOT.SYSKD,0) > 0
              THEN KD.VORNAME
              || ' '
              || KD.NAME
              ELSE IT.VORNAME
              || ' '
              || IT.NAME
              END KUNDE,
              CASE
              WHEN NVL(ANGEBOT.SYSKD,0) > 0
              THEN KD.PLZ
              ||' '
              ||KD.ORT
              ELSE IT.PLZ
              ||' '
              ||IT.ORT
              END ORT,
              IT.SYSIT,
              ANGKALK.AUSZAHLUNG SUMME,
              ANGEBOT.SYSABWICKLUNG,
              ANGOB.BEZEICHNUNG OBJEKTBEZ,
              ANGEBOT.SYSCAMP,
              BPLISTENER.syswfuser,
              angebot.erfassungsclient,
              reminder.startdate faellig,
              peoption.option2 segment,peoption.str02 kategorie,camp.name kampagne,camp.syscamp kampagneid,reminder.syswfuser RSYSWFUSER,angebot.sysprchannel ,awo.name ABWICKLUNGSORT,
              ANGEBOT.SYSVK sysbearbeiter,
              ANGEBOT.SYSBERATER
              FROM CIC.BPLISTENER,
              CIC.BPPROCDEF,
              CIC.bpeventdef evt,
              CIC.bpevaldef ev,
              CIC.ANGEBOT ANGEBOT,
              CIC.VART VART,
              CIC.VTTYP,
              CIC.ANGOB ANGOB,
              CIC.PERSON VM,
              CIC.PERSON VK,
              CIC.ANGKALK ANGKALK,
              CIC.PRPRODUCT PRPRODUCT,
              CIC.PERSON KD,
              CIC.IT IT,
              CIC.ANGOPTION ANGOPTION,
              cic.reminder reminder,

              cic.peoption peoption,
              CIC.camp camp,
              CIC.WFUSER bt,
              CIC.PEROLE AWO,
              CIC.CTLANG,
              cic.wfuser usr
              WHERE BPLISTENER.SYSBPPROCDEF       = BPPROCDEF.SYSBPPROCDEF(+)
              AND (BPLISTENER.SUPERVISORFLAGS   =0
              OR BPLISTENER.SUPERVISORFLAGS    >=16)
              AND BPLISTENER.ISUSERTASK         = 1
              AND NVL(BPLISTENER.EXECSTATUS, 0) = 0
              AND BPLISTENER.evaluatecode=ev.evaluatecode(+) and BPLISTENER.eventcode=evt.eventcode(+)
              and (
              (BPLISTENER.OLTABLE            = 'ANGEBOT'  AND BPLISTENER.SYSOLTABLE         = ANGEBOT.SYSID)
              or  (BPLISTENER.OLTABLE            = 'REMINDER' and ANGEBOT.sysid=reminder.sysid)

              )
              and reminder.area(+)='ANGEBOT'  AND BPLISTENER.SYSOLTABLE         = reminder.sysreminder(+)

              AND (VART.SYSVART (+)             = PRPRODUCT.SYSVART
              AND ANGEBOT.SYSVTTYP      = VTTYP.SYSVTTYP(+)
              AND ANGKALK.SYSPRPRODUCT          = PRPRODUCT.SYSPRPRODUCT (+)
              AND ANGEBOT.SYSID                 = ANGKALK.SYSANGEBOT(+))
              AND (ANGOB.SYSOB(+)               = ANGKALK.SYSOB)
              AND (ANGEBOT.SYSVM                = VM.SYSPERSON(+))
              AND (ANGEBOT.SYSVM                = PEOPTION.SYSID(+))
              AND (ANGEBOT.SYSVK                = VK.SYSPERSON(+))
              AND (ANGEBOT.SYSKD                = KD.SYSPERSON(+))
              AND (ANGEBOT.SYSIT                = IT.SYSIT(+))
              AND (ANGEBOT.SYSCAMP              = CAMP.SYSCAMP(+))
              AND (ANGEBOT.SYSID                = ANGOPTION.SYSID(+))
              AND (ANGEBOT.SYSBERATER                = BT.SYSWFUSER(+))
              AND (ANGEBOT.SYSABWICKLUNG                 = AWO.SYSPEROLE(+))
              and CTLANG.SYSCTLANG(+) = IT.SYSCTLANG
              and bplistener.syswfuser=usr.syswfuser
              ) bplistener where 1=1 {FILTERCONDITIONS}-->
            

          </query>
        </viewmeta>
      </customentry>



