          <query>

            <query>
              select * from (
              SELECT usr.vorname||' '||usr.name benutzer,CIC.CIC_SYS.to_oradate(BPLISTENER.createdcd) datum,BPLISTENER.SYSBPPROCINST,
              BPLISTENER.EVENTCODE,
              BPLISTENER.SYSBPLISTENER sysbplistener ,
              BPPROCDEF.DESCRIPTION,
              BPPROCDEF.PROCESSDEFCODE,
              coalesce(ev.description,evt.description) STEPDESCRIPTION,
              bplistener.oltable,
              BPLISTENER.SYSOLTABLE,
              BPLISTENER.NAMEBPLANE namebprole,
              BPLISTENER.EVALUATECODE,
              ANTRAG.ANTRAG NUMMER,
              ANTRAG.SYSKD,
              ANTRAG.SYSIT

              FROM
              CIC.BPLISTENER,
              CIC.BPPROCDEF,
              CIC.bpeventdef evt,
              CIC.bpevaldef ev,
              CIC.ANTRAG ANTRAG,
              cic.reminder reminder,
              cic.wfuser usr
              WHERE BPLISTENER.SYSBPPROCDEF       = BPPROCDEF.SYSBPPROCDEF(+)
              AND BPLISTENER.ISUSERTASK         = 1
              AND NVL(BPLISTENER.EXECSTATUS, 0) = 0
              AND (BPLISTENER.SUPERVISORFLAGS   =0
              OR BPLISTENER.SUPERVISORFLAGS    >=16)
              AND BPLISTENER.evaluatecode=ev.evaluatecode(+) and BPLISTENER.eventcode=evt.eventcode(+)
              and (
              (BPLISTENER.OLTABLE            = 'ANTRAG'  AND BPLISTENER.SYSOLTABLE         = ANTRAG.SYSID)
              or  (BPLISTENER.OLTABLE            = 'REMINDER' and antrag.sysid=reminder.sysid)

              )
              and reminder.area(+)='ANTRAG'  AND BPLISTENER.SYSOLTABLE         = reminder.sysreminder(+)
              and bplistener.syswfuser=usr.syswfuser
              union all
              SELECT usr.vorname||' '||usr.name benutzer,CIC.CIC_SYS.to_oradate(BPLISTENER.createdcd) datum,BPLISTENER.SYSBPPROCINST,
